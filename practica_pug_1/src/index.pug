include components/data.pug
include components/mixin.pug
doctype html
html(lang="en")
  head
    title #{title}
  body 
    header
        h2= title
    nav
        +createMenu(dictMenu, 'Главная')

    div.images 
         each pict in listImages 
                img(src= require("./images/" + pict))

    article
         +sectionBlock(listSections)

    footer
        h4 &copy; Пример html-страницы
